    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "react-app-rewired": "^2.2.1",
    "terser-webpack-plugin": "5.3.14",
    "vite": "4.5.2",
    "vite-plugin-environment": "1.1.3",
    "webpack-bundle-analyzer": "4.10.2"
  }
